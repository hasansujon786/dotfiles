  "copyFormatting": "none",
  "copyOnSelect": true,
  "defaultProfile": "{0caa0dad-35be-5f56-a8ff-afceeeaa6102}",
  "initialCols": 120,
  "initialPosition": "80,30",
  "initialRows": 28,
  "largePasteWarning": false,
  "launchMode": "default",
  "multiLinePasteWarning": false,
  "snapToGridOnResize": true,
  "theme": "system",
  "useAcrylicInTabRow": true,
  "actions": [
    { "command": "unbound", "keys": "alt+space" },
    { "command": { "action": "copy", "singleLine": false }, "keys": "ctrl+shift+c" },
    { "command": "paste", "keys": "ctrl+shift+v" },
    { "command": "toggleFocusMode", "keys": "alt+t" },
    { "command": "toggleFullscreen", "keys": "alt+enter" },
    { "command": "find", "keys": "ctrl+shift+f" },
    { "command": { "action": "nextTab" }, "keys": "alt+`" },
    { "command": { "action": "nextTab" }, "keys": "alt+]" },
    { "command": { "action": "prevTab" }, "keys": "alt+shift+`" },
    { "command": { "action": "prevTab" }, "keys": "alt+[" },
    { "command": "tabSearch", "keys": "alt+\\" },
    { "command": "openTabRenamer", "keys": "alt+f2" },
    { "command": { "action": "moveTab", "direction": "backward" }, "keys": "alt+shift+[" },
    { "command": { "action": "moveTab", "direction": "forward" }, "keys": "alt+shift+]" },
    { "command": { "action": "splitPane", "split": "down" }, "keys": "alt+shift+s" },
    { "command": { "action": "splitPane", "split": "right" }, "keys": "alt+shift+v" },
    { "command": { "action": "splitPane", "split": "auto", "splitMode": "duplicate" }, "keys": "alt+shift+d" },
    { "command": { "action": "resizePane", "direction": "left" }, "keys": "alt+shift+comma" },
    { "command": { "action": "resizePane", "direction": "down" }, "keys": "alt+shift+minus" },
    { "command": { "action": "resizePane", "direction": "up" }, "keys": "alt+shift+plus" },
    { "command": { "action": "resizePane", "direction": "right" }, "keys": "alt+shift+period" },
    { "command": { "action": "moveFocus", "direction": "right" }, "keys": "alt+shift+l" },
    { "command": { "action": "moveFocus", "direction": "left" }, "keys": "alt+shift+h" },
    { "command": { "action": "moveFocus", "direction": "down" }, "keys": "alt+shift+j" },
    { "command": { "action": "moveFocus", "direction": "up" }, "keys": "alt+shift+k" },
    { "command": "scrollUp", "keys": "alt+pageup" },
    { "command": "scrollDown", "keys": "alt+pagedown" },

    { "command": { "action": "sendInput", "input": "\u0011" }, "keys": "ctrl+space" },
    { "command": { "action": "sendInput", "input": "\u0017" }, "keys": "ctrl+backspace" },
    { "command": { "action": "sendInput", "input": "\u001b[105;5u" }, "keys": "ctrl+i" },
    { "command": { "action": "sendInput", "input": "\u001b[109;5u" }, "keys": "ctrl+m" },
    { "command": { "action": "sendInput", "input": "\u001b[13;5u" }, "keys": "ctrl+enter" }
  ],
