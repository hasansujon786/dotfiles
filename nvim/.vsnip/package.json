{
  "name": "my-custom-snip",
  "engines": {
    "vscode": "^1.11.0"
  },
  "contributes": {
    "snippets": [
      {
        "language": [
          "plaintext",
          "markdown",
          "tex",
          "html",
          "global",
          "all"
        ],
        "path": "./global.json"
      },
      {
        "language": [
          "javascript",
          "javascriptreact"
        ],
        "path": "./javascriptreact.json"
      },
      {
        "language": "dart",
        "path": "./dart.json"
      },
      {
        "language": "lua",
        "path": "./lua.json"
      }
    ]
  }
}
