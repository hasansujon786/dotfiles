{
  "Stateless Widget": {
    "prefix": "stlessW",
    "body": [
      "class ${1:MyStatelessWidget} extends StatelessWidget {",
      "  const ${1:MyStatelessWidget}({Key? key}) : super(key: key);\n",
      "  @override",
      "  Widget build(BuildContext context) {",
      "    return Container();",
      "  }",
      "}"
    ],
    "description": "Create a Stateless widget"
  },
  "Stateful Widget": {
    "prefix": "stfulW",
    "body": [
      "class ${1:MyStatefulWidget} extends StatefulWidget {",
      "  const ${1:MyStatefulWidget}({Key? key}) : super(key: key);\n",
      "  @override",
      "  State<${1:WidgetName}> createState() => _${1:WidgetName}State();",
      "}\n",
      "class _${1:WidgetName}State extends State<${1:WidgetName}> {",
      "  @override",
      "  Widget build(BuildContext context) {",
      "    return Container();",
      "  }",
      "}"
    ],
    "description": "Create a Stateful widget"
  },
  "Material App Scaffold Stateless": {
    "prefix": "sfless",
    "description": "Scaffold a Material Screen",
    "body": [
      "import 'package:flutter/material.dart';\n",
      "class ${1:MyStatelessWidget} extends StatelessWidget {",
      "  const ${1:MyStatelessWidget}({Key? key}) : super(key: key);\n",
      "  static const routeName = '/${2:route_name}';",
      "  @override",
      "  Widget build(BuildContext context) {",
      "    return Scaffold(",
      "      appBar: AppBar(",
      "        title: const Text('${1:Screen Title}'),",
      "      ),",
      "      body: const Center(",
      "        child: Text('Hello World'),",
      "      ),",
      "    );",
      "  }",
      "}"
    ]
  },
  "Material App Scaffold Stateful": {
    "prefix": "sfful",
    "description": "Scaffold a Material Screen",
    "body": [
      "import 'package:flutter/material.dart';\n",
      "class ${1:MyStatefulWidget} extends StatefulWidget {",
      "  const ${1:MyStatefulWidget}({Key? key}) : super(key: key);\n",
      "  static const routeName = '/${2:route_name}';",
      "  @override",
      "  State<${1:WidgetName}> createState() => _${1:WidgetName}State();",
      "}\n",
      "class _${1:index}State extends State<${1:index}> {",
      "  @override",
      "  Widget build(BuildContext context) {",
      "    return Scaffold(",
      "      appBar: AppBar(",
      "        title: const Text('${1:Screen Title}'),",
      "      ),",
      "      body: const Center(",
      "        child: Text('Hello World'),",
      "      ),",
      "    );",
      "  }",
      "}"
    ]
  },
